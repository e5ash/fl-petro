							.quality__desc Гарантируем плотное сцепление с поверхностью #[br]за счет максимального давления в 30 бар
					.de__photos.row
						a.de__photo(href="javascript:;" data-fancybox="" data-src="")
							img(src="" alt="")
						a.de__photo(href="javascript:;" data-fancybox="" data-src="")
							img(src="" alt="")
		.de__img
			img(src="" alt="")


	.bg-wrap
		.economy
			.case
				.economy__title.h1 #[strong сэкономим вам #[br]до 30%] на материалах
				.economy__subtitle закупаем штукатурку напрямую с завода по оптовым #[br]ценам и #[strong храним на собственном складе]
				.economy__wrap.row
					.economy__item
						.economy__icon
							img(src="" alt="")
						.economy__text #[strong Подтверждаем сертификатами,] #[br]что мы напрямую с завода #[br]закупаем материалы
						.economy__logos.row
