extends ../../templates/default
block vars
	-
		title = "Главная страница"
		breadcrumbs = [
			['Главная', 'index.html'],
			[title]
		]
block content
	.intro.bg-wrap
		.case
			.intro__content
				.intro__title.h1 Механизированная #[br]#[strong штукатурка] #[u от 300 р/м#[sup 2]]
				.intro__subtitle.h2 в Москве и МО с гарантией лучшей цены
				.intro__materials.b-materials
					.b-materials__img
						img(src="" alt="")
					.b-materials__text
						p Закупаем материал напрямую с завода без дилерских #[br]наценок, что #[strong дает экономию до 30%.] Не перепродаем #[br]заявки, а сами реализуем работу — #[strong все специалисты #[br]в штате, а техника в собственности у компании.]
				a.intro__btn.btn.--md.--bg-blue() РАССЧИТАТЬ СТОИМОСТЬ
			.intro__count.round
				.round__wrap
					.round__img
						img(src="" alt="")
					.round__text Оштукатурили 1500+ #[br]домов с 2018 года

			.intro__img
				img(src="" alt="")

		.intro__bg.bg
			img(src="" alt="")


	.advantages
		.case
			.advantages__wrap.b
				.advantages__list
					.advantages__item
						.advantages__icon
							img(src="" alt="")
						.advantages__subtitle.h4 Сэкономим вам до 30% #[br]на материалах
						.advantages__text Покупаем по оптовым ценам штукатурку #[br]напрямую с завода и храним на своём складе

					.advantages__item
						.advantages__icon
							img(src="" alt="")
						.advantages__subtitle.h4 Соблюдаем #[br]СНиП и ГОСТ
						.advantages__text При необходимости подготовим полный #[br]комплект исполнительной документации

					.advantages__item
						.advantages__icon
							img(src="" alt="")
						.advantages__subtitle.h4 Выходим на объект #[br]в день заказа
						.advantages__text Бригады заезжают на объект #[br]в день подписания договора

					.advantages__item
						.advantages__icon
							img(src="" alt="")
						.advantages__subtitle.h4 Штукатурим любые #[br]поверхности
						.advantages__text Стены, потолки, откосы, фасады, #[br]а также делаем полусухую стяжку пола

					.advantages__item
						.advantages__icon
							img(src="" alt="")
						.advantages__subtitle.h4 Гарантия #[br]3 года
						.advantages__text Если со штукатуркой будут проблемы, #[br]исправим их за свой счет

					.advantages__item
						.advantages__icon
							img(src="" alt="")
						.advantages__subtitle.h4 Поэтапная #[br]оплата
						.advantages__text При заезде рабочих на объект вносите #[br]предоплату 30% по договору. Остальную #[br]часть после завершения всех работ

	.best-price.bg
		.case
			.best-price__person.person
				.person__icon
					img(src="" alt="")
				.person__name ПЕТР #[br]НИКИТИН
				.person__position Руководитель #[br]компании
			.best-price__img
				img(src="" alt="")
			.best-price__content
				.best-price__title.h1 Даем гарантию #[strong лучшей цены]
				.best-price__subtitle.bi
					.bi__icon
						img(src="" alt="")
					.bi__text.h2 Если найдете дешевле, мы #[br]снизим цену до -10%
				.best-price__quote.quote
					h3 Почему мы гарантируем самую низкую цену
					p Мы закупаем материал напрямую с завода без дилерских наценок, #[br]что дает экономию до 30%. Не перепродаем заявки, а сами реализуем #[br]работу — все специалисты в штате, а штукатурные станции #[br]и автомобили в собственности у компании.
				.best-price__foot
					a.best-price__btn.btn.--md.--bg-blue(href="") РАССЧИТАТЬ СТОИМОСТЬ
					.best-price__btn-desc В феврале делаем максимальную скидку #[br]и доставляем материалы за свой счет

		.best-price__img
			img(src="" alt="")


	.surface
		.case
			.surface__title.h1 ШТУКАТУРИМ #[strong ЛЮБЫЕ #[br]ПОВЕРХНОСТИ]
			.surface__list.row
				.surface__item
					.surface__img
						img(src="" alt="")
					.surface__name МЕТАЛЛОКОНСТРУКЦИИ

				.surface__item
					.surface__img
						img(src="" alt="")
					.surface__name БЕТОН

				.surface__item
					.surface__img
						img(src="" alt="")
					.surface__name ОКРАШЕННЫЕ ПОВЕРХНОСТИ

				.surface__item
					.surface__img
						img(src="" alt="")
					.surface__name КИРПИЧ

				.surface__item
					.surface__img
						img(src="" alt="")
					.surface__name Газо- и пенобетон

				.surface__item
					.surface__img
						img(src="" alt="")
					.surface__name и другие поверхности


	.price
		.case
			.price__title.h1 СТОИМОСТЬ ОТ #[strong 300 р/м#[sup 2]]
			.price__subtitle.h3 Это минимальная цена без материалов при объеме от 200 м#[sup 2]
			.price__list.row
				mixin pItem(img, name, price, text)
					.price__item.p-item
						.p-item__head
							.p-item__img
								img(src=img alt="")
							.p-item__title ТАРИФ
							.p-item__name= name
						.p-item__body
							.p-item__text.t2= text
							.p-item__price.p
								.p__title СТОИМОСТЬ
								.p__value от #[var= price] РУБ/м#[sup 2]
								.p__tog материалы
						.p-item__foot.feedback
							.feedback__title Рассчитайте точную стоимость #[br]по телефону за 2 минуты
							.feedback__text Менеджер задаст уточняющие вопросы #[br]и сделает скидку в зависимости от объема
							.feedback__input.input.--icon.--phone.--required
								.input__wrap
									input.input__area(type="number" name="phone" placeholder="+7 ( ___ ) ___ - __ - __")
								.input__icon
							button.feedback__btn.btn.--md.--bg-blue РАССЧИТАТЬ СТОИМОСТЬ
							.feedback__messengers.messengers
								.messengers__icon
									img(src="" alt="")
								.messengers__link #[a(href="") ИСПОЛЬЗОВАТЬ МЕССЕНДЖЕРЫ]
				+pItem('', 'МИНИМАЛЬНЫЙ', '300', '<h3>Что включено:</h3><ul><li>Грунтование</li><li>Выставление маяков</li><li>Оштукатуривание стен</li><li>Уборка мусора</li></ul>')
				+pItem('', 'СТАНДАРТ', '350', '<h3>Что включено:</h3><ul><li>Грунтование</li><li>Выставление маяков</li><li>Оштукатуривание стен</li><li>Затирка и глянцевание</li><li>Использование оцинкованных уголков</li><li>Уборка мусора</li></ul>')
				+pItem('', 'МАКСИМАЛЬНЫЙ', '390', '<h3>Что включено:</h3><ul><li>Грунтование</li><li>Выставление маяков</li><li>Оштукатуривание стен</li><li>Затирка и глянцевание под обои</li><li>Использование ПВХ уголков </li><li>и профиля примыкания</li><li>Уборка мусора</li><li>Ровные углы 90<sup>о</sup></li></ul>')
			.price__desc.h3 Включена доставка оборудования до 40 км


	.allinc
		.case
			.allinc__subtitle.h2 Вам не нужно будет ни за что доплачивать
			.allinc__title.h1 В МАКСИМАЛЬНОм ТАРИФЕ #[br]#[strong ВКЛЮЧЕНЫ ВСЕ работы] #[br]«под ключ»
			.allinc__list.row
				.allinc__item
					.allinc__name Грунтовка стен
					.allinc__value 01
				.allinc__item
					.allinc__name Монтаж сетки
					.allinc__value 02
				.allinc__item
					.allinc__name Установка уголков
					.allinc__value 03
				.allinc__item
					.allinc__name Уборка мусора
					.allinc__value 04
				.allinc__item
					.allinc__name Установка и демонтаж #[br]маяков
					.allinc__value 05
				.allinc__item
					.allinc__name Доставка материалов #[br]и оборудования
					.allinc__value 06
				.allinc__item
					.allinc__name Заглаживание, #[br]глянцевание
					.allinc__value 07
				.allinc__item
					.allinc__name Нанесение #[br]штукатурки
					.allinc__value 08
			.allinc__img
				img(src="" alt="")

	.calc
		.case
			.calc__block.b
				.calc__intro
					.calc__subtitle.h2 ответьте на 5 вопросов
					.calc__title.h1 #[strong и рассчитайте стоимость оштукатуривания за 2 минуты] 
					.calc__desc В 3-х вариантах: «Минимальный», «Под ключ», «Премиум»
					.calc__wrap
						.calc__status.status
							.status__item
						.calc__list
							.calc__item
								.calc__radios.row
									.calc__radio.rad-img.check
										input.rad-img__input(type="radio" name="" value="" checked="checked")
										.rad-img__wrap
											.rad-img__inner
												.rad-img__icon
												.rad-img__title СТЕНЫ
											.rad-img__img
												img(src="" alt="")
									.calc__radio.rad-img.check
										input.rad-img__input(type="radio" name="" value="")
										.rad-img__wrap
											.rad-img__inner
												.rad-img__icon
												.rad-img__title Откосы
											.rad-img__img
												img(src="" alt="")
									.calc__radio.rad-img.check
										input.rad-img__input(type="radio" name="" value="")
										.rad-img__wrap
											.rad-img__inner
												.rad-img__icon
												.rad-img__title фасады
											.rad-img__img
												img(src="" alt="")
									.calc__radio.rad-img.check
										input.rad-img__input(type="radio" name="" value="")
										.rad-img__wrap
											.rad-img__inner
												.rad-img__icon
												.rad-img__title потолки
											.rad-img__img
												img(src="" alt="")
							.calc__controls
								.calc__control.btn.--md.--bg-blue.--prev
									span ПРЕДЫДУЩИЙ ШАГ
								.calc__control.btn.--md.--bg-blue.--next
									span СЛЕДУЮЩИЙ ШАГ

				.calc__gifts.b-gifts
					.b-gifts__head
						.b-gifts__title до 17 февраля #[br]#[strong подарок на выбор]
						.b-gifts__subtitle после прохождения теста
					.b-gifts__list
						.b-gifts__item
							.b-gifts__name Вывоз мусора
							.b-gifts__img
								img(src="" alt="")
						.b-gifts__item
							.b-gifts__name 5 мешков штукатурки #[br]для заделывания штроб
							.b-gifts__img
								img(src="" alt="")
						.b-gifts__item
							.b-gifts__name Штукатурка откосов #[br]двух окон
							.b-gifts__img
								img(src="" alt="")
						.b-gifts__item
							.b-gifts__name Утепление #[br]одного окна
							.b-gifts__img
								img(src="" alt="")

	.oneday.bg-wrap
		.case
			.oneday__subtitle.h2 ЗА ОДИН ДЕНЬ
			.oneday__title.h1 оштукатурим от 70 до 100 м#[sup 2]
			.oneday__list.row
				.oneday__item
					.oneday__text #[strong В 3 раза быстрее,] #[br]чем если бы вы заказывали #[br]ручную штукатурку
					.oneday__icon
						img(src="" alt="")
				.oneday__item
					.oneday__text #[strong До 40% выгоднее] #[br]по сравнению с ручным #[br]способом за счет автоматики
					.oneday__icon
						img(src="" alt="")
				.oneday__item
					.oneday__text #[strong Стены не требуют шпаклевания] #[br]- после нашей работы #[br]поверхности гладкие и ровные
					.oneday__icon
						img(src="" alt="")
			.oneday__img
				img(src="" alt="")
			.oneday__round.round
				.round__wrap
					.round__img
						img(src="" alt="")
					.round__text
						На объекте работает #[br]#[strong команда от 3 до 8 человек] #[br]под контролем старшего #[br]бригадира

		.oneday__bg.bg
			img(src="" alt="")



	.fix
		.case
			.fix__wrap
				.fix__content
					.fix__subtitle.h2 Если штукатурка потрескается #[br]или произойдут другие проблемы,
					.fix__title.h1 мы исправим #[br]все #[strong за свой счет] 
					.fix__warranty
						.warranty__title По договору гарантируем:
						.warranty__list.row
							.warranty__item Гладкие стены под подраску
							.warranty__item Ровные углы 90#[sup °]
							.warranty__item Поверхности без дефектов и перепадов
							.warranty__item Стены без трещин
					.fix__img
						img(src="" alt="")
					.fix__count
						.fix__count-icon
							img(src="" alt="")
						.fix__count-value Выдаем #[br]гарантию #[br]3 года
			.fix__list.row
				.fix__item
					.fix__icon
						img(src="" alt="")
					.fix__name Соблюдаем СНиП и ГОСТ
					.fix__text При необходимости подготовим #[br]полный комплект исполнительной #[br]документации
				.fix__item
					.fix__icon
						img(src="" alt="")
					.fix__name Подписываем договор 
					.fix__text Мы прописали в договоре #[br]каждую мелочь, чтобы #[br]обезопасить вас #[br]#[a(href="") Посмотреть договор]
				.fix__item
					.fix__icon
						img(src="" alt="")
					.fix__name Берем аванс 30%
					.fix__text при заезде бригады на объект — #[br]остальная часть после приемки
				.fix__item
					.fix__icon
						img(src="" alt="")
					.fix__name Укладываемся в срок
					.fix__text или платим штраф 2500₽ #[br]за каждый день задержки


	.ready
		.case
			.ready__wrap
				.ready__img
					img(src="" alt="")
				.ready__content
					.ready__subtitle.h2 Готовы начать штукатурить?
					.ready__title.h1 #[strong Вызовите замерщика #[br]бесплатно] в удобное #[br]для вас время
					.ready__list.row
						.ready__item
							.ready__icon
								img(src="" alt="")
							.ready__text Вы получите #[strong точный расчет #[br]стоимости и план работ]
						.ready__item
							.ready__icon
								img(src="" alt="")
							.ready__text Гарантируем, что #[strong смета #[br]не изменится ни на 1 рубль]
						.ready__item.--full
							.ready__icon
								img(src="" alt="")
							.ready__text Специалист #[strong ответит на все вопросы] и подскажет, на какие #[br]нюансы необходимо обратить внимание в вашем случае
			.ready__form.form.--line
				.form__inner
					.form__title Заполните форму ниже, чтобы вызвать замерщика
					.form__wrap
						.form__input.input.--md.--bg-gray.--icon.--phone.--required
							.input__wrap
								input.input__area(type="number" name="phone" placeholder="+7 ( ___ ) ___ - __ - __")
							.input__icon
						button.form__btn.btn.--md.--bg-blue РАССЧИТАТЬ СТОИМОСТЬ
					.form__policy Нажимая на кнопку, вы даете согласие на обработку своих персональных #[br]данных и соглашаетесь с политикой конфиденциальности.

	.whatsapp
		.case
			.whatsapp__title.h1 НЕт ВРЕМЕНИ #[br]НА ЗВОНКИ?
			.whatsapp__img
				img(src="" alt="")
			.whatsapp__content
				.whatsapp__subtitle НАПИШИТЕ НАМ В #[img(src="" alt="")] whatsapp в один клик
				a.whatsapp__btn.btn.--md.--bg-green(href="") НАПИСАТЬ В WHATSAPP


	.de.bg-wrap
		.case
			.de__wrap.row
				.de__img-wrap
					.de__img
						img(src="" alt="")
					.de__round.round
						.round__wrap
							.round__img
								img(src="" alt="")
							.round__text
								p 13 штукатурных #[br]станций PFT Ritmo XL
								p Стоимость одной - #[br]700 000 руб.
				.de__content
					.de__title.h1 используем #[br]#[img(src="" alt="")] #[strong немецкое #[br]оборудование]
					.de__subtitle.h2 которое сэкономит ваш бюджет #[br]при чистовых работах
					.de__quality.quality
