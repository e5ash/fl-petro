.objects
	.case
		.objects__title.h1 Оштукатурили #[br]больше #[strong 500 объектов]
		.objects__subtitle.h2 домов, квартир и коммерческих помещений
		.objects__list
			mixin object
				.objects__item.object
					.object__content
						.object__name Механизированная штукатурка стен, откосов в загородном доме 800 м#[sup 2]
						.object__data.data
							.data__list.row
								.data__item
									.data__icon
											img(src="_/uploads/tape-green.svg" alt="")
									.data__text
										.data__title Площадь по полу: 
										.data__value 1000 м#[sup 2]
								.data__item
									.data__icon
											img(src="_/uploads/tape-green.svg" alt="")
									.data__text
										.data__title Площадь по стенам: 
										.data__value 4000 м#[sup 2]
								.data__item
									.data__icon
											img(src="_/uploads/clock-green.svg" alt="")
									.data__text
										.data__title СРОК: 
										.data__value 4 дня
								.data__item.--full
									.data__icon
										img(src="_/uploads/info-green.svg" alt="") 
									.data__text
										.data__title Описание:
										.data__value Выполнили работу в жилой квартире площадью 80 кв. м #[br]за 1 день. Заказчик остался доволен проделанной работой. #[br]Дополнительно залили полусухую стяжку.
						.object__btns.row
							a.object__btn.btn.--md.--bg-blue(href="javascript:;" data-fancybox="" data-src="#modal-calc") У МЕНЯ ПОХОЖАЯ ЗАДАЧА
							a.object__more(href="object.html")
								img(src="_/uploads/info-gray.svg" alt="")
								span Узнать подробнее
					.object__imgs.imgs
						.imgs__list
							.imgs__list-wrap.swiper-container
								.imgs__list-list.swiper-wrapper
									.imgs__list-item.swiper-slide
										img(src="_/uploads/object-1.png" alt="")
									.imgs__list-item.swiper-slide
										img(src="_/uploads/object-1.png" alt="")
									.imgs__list-item.swiper-slide
										img(src="_/uploads/object-1.png" alt="")
									.imgs__list-item.swiper-slide
										img(src="_/uploads/object-1.png" alt="")

						.imgs__thumbs
							.imgs__thumbs-wrap.swiper-container
								.imgs__thumbs-list.swiper-wrapper
									.imgs__thumbs-item.swiper-slide
										img(src="_/uploads/object-1.png" alt="")
									.imgs__thumbs-item.swiper-slide
										img(src="_/uploads/object-1.png" alt="")
									.imgs__thumbs-item.swiper-slide
										img(src="_/uploads/object-1.png" alt="")
									.imgs__thumbs-item.swiper-slide
										img(src="_/uploads/object-1.png" alt="")
			+object
			+object
			+object

